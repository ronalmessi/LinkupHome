<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideBar">
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="actual_setting" format="integer" />
        <attr name="desired_setting" format="integer" />
        <attr name="range" format="integer" />
    </declare-styleable>

    <declare-styleable name="ViewPagerWithIndicator">
        <attr name="arrow.enable" format="boolean|reference" />
        <attr name="arrow.width" format="dimension|reference" />
        <attr name="arrow.height" format="dimension|reference" />
        <attr name="left_arrow.drawable" format="reference" />
        <attr name="right_arrow.drawable" format="reference" />

    </declare-styleable>

    <attr name="checkedTextColor" format="color" />
    <attr name="uncheckedTextColor" format="color" />
    <attr name="buttonBackground" format="reference" />
    <attr name="checkedBackground" format="reference" />
    <attr name="animationType" format="enum">
        <enum name="none" value="0" />
        <enum name="scale" value="1" />
        <enum name="alpha" value="2" />
    </attr>
    <attr name="animationDuration" format="integer" />

    <declare-styleable name="ToggleButtonGroup">
        <attr name="android:textSize" />
        <attr name="android:saveEnabled" />
        <attr name="checkedTextColor" />
        <attr name="uncheckedTextColor" />
        <attr name="checkedBackground" />
        <attr name="buttonBackground" />
        <attr name="animationType" />
        <attr name="animationDuration" />
        <attr name="buttonWidth" format="dimension" />
        <attr name="buttonHeight" format="dimension" />
        <attr name="textButton1" format="string" />
        <attr name="textButton2" format="string" />
        <attr name="textButtons" format="reference" />
        <attr name="buttonSpacing" format="enum|dimension">
            <enum name="auto" value="-65536" />
        </attr>
        <attr name="buttonSpacingForLastRow" format="enum|dimension">
            <enum name="auto" value="-65536" />
            <enum name="align" value="-65537" />
        </attr>
        <attr name="rowSpacing" format="enum|dimension">
            <enum name="auto" value="-65536" />
        </attr>
        <attr name="wrap" format="boolean" />

    </declare-styleable>

    <declare-styleable name="ToggleButton">
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="checkedTextColor" />
        <attr name="uncheckedTextColor" />
        <attr name="checkedBackground" />
        <attr name="buttonBackground" />
        <attr name="animationType" />
        <attr name="animationDuration" />
    </declare-styleable>

    <declare-styleable name="LoopView">
        <attr name="lineColor" format="color" />
        <attr name="topBottomTextColor" format="color" />
        <attr name="centerTextColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="canLoop" format="boolean" />
        <attr name="initPosition" format="integer" />
        <attr name="drawItemCount" format="integer" />
    </declare-styleable>

    <declare-styleable name="DashboardView">
        <attr name="padding" format="dimension" />
        <attr name="progressStrokeWidth" format="dimension" />
        <attr name="android:text" />
        <attr name="unit" format="string" />
        <attr name="android:textSize" />
        <attr name="textColor" format="color" />
        <attr name="startNumber" format="integer" />
    </declare-styleable>


    <declare-styleable name="DonutProgress">
        <attr name="donut_progress" format="integer" />
        <attr name="donut_max" format="integer" />

        <attr name="donut_unfinished_color" format="color" />
        <attr name="donut_finished_color" format="color" />

        <attr name="donut_finished_stroke_width" format="dimension" />
        <attr name="donut_unfinished_stroke_width" format="dimension" />

        <attr name="donut_text_size" format="dimension" />
        <attr name="donut_text_color" format="color" />

        <attr name="donut_prefix_text" format="string" />
        <attr name="donut_suffix_text" format="string" />
        <attr name="donut_text" format="string" />

        <attr name="donut_background_color" format="color" />

        <attr name="donut_inner_bottom_text" format="string" />
        <attr name="donut_inner_bottom_text_size" format="dimension" />
        <attr name="donut_inner_bottom_text_color" format="color" />

        <attr name="donut_circle_starting_degree" format="integer" />
        <attr name="donut_show_text" format="boolean" />
        <attr name="donut_inner_drawable" format="reference" />
    </declare-styleable>


    <declare-styleable name="WheelPicker">
        <attr name="wheel_data" format="reference" />
        <attr name="wheel_selected_item_position" format="integer" />
        <attr name="wheel_item_text_size" format="dimension" />
        <attr name="wheel_item_text_color" format="color" />
        <attr name="wheel_selected_item_text_color" format="color" />
        <attr name="wheel_same_width" format="boolean" />
        <attr name="wheel_maximum_width_text" format="string" />
        <attr name="wheel_maximum_width_text_position" format="integer" />
        <attr name="wheel_visible_item_count" format="integer" />
        <attr name="wheel_item_space" format="dimension" />
        <attr name="wheel_cyclic" format="boolean" />
        <attr name="wheel_indicator" format="boolean" />
        <attr name="wheel_indicator_color" format="color" />
        <attr name="wheel_indicator_size" format="dimension" />
        <attr name="wheel_curtain" format="boolean" />
        <attr name="wheel_curtain_color" format="color" />
        <attr name="wheel_atmospheric" format="boolean" />
        <attr name="wheel_curved" format="boolean" />
        <attr name="wheel_item_align" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="wheel_font_path" format="string" />
    </declare-styleable>


    <declare-styleable name="Thermometer">
        <attr name="radius" format="dimension" />
        <attr name="middleColor" format="color" />
        <attr name="innerColor" format="color" />
    </declare-styleable>


    <declare-styleable name="TimerSettingView">
        <attr name="mTextSize" format="dimension" />
        <attr name="reached_color" format="color" />
        <attr name="unreached_color" format="color" />
        <attr name="hour_reached_width" format="dimension" />
        <attr name="hour_unreached_width" format="dimension" />
        <attr name="minute_line_width" format="dimension" />
        <attr name="minute_line_length" format="dimension" />
        <attr name="minute_long_line_length" format="dimension" />
        <attr name="slider_radius" format="dimension" />
        <attr name="slider_inner_radius" format="dimension" />
        <attr name="slider_bg_color" format="color" />
        <attr name="slider_inner_bg_color" format="color" />
        <attr name="circle_gap" format="dimension" />
    </declare-styleable>


    <declare-styleable name="BedWarmColdCircleView">
        <attr name="circle_width" format="dimension" />
        <attr name="arrow_gap" format="dimension" />
        <attr name="text_size" format="dimension" />
        <attr name="mTextColor" format="color" />
    </declare-styleable>


</resources>
