<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/informFragment">


    <fragment
        android:id="@+id/informFragment"
        android:name="com.ihomey.linkuphome.inform.InformFragment"
        android:label="InformFragment"
        tools:layout="@layout/inform_fragment">

        <action
            android:id="@+id/action_informFragment_to_webViewFragment"
            app:destination="@id/webViewFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/webViewFragment"
        android:name="com.ihomey.linkuphome.setting.WebViewFragment"
        android:label="WebViewFragment"
        tools:layout="@layout/webview_fragment" />


</navigation>