<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/tab_devices">

    <fragment
        android:id="@+id/tab_devices"
        android:name="com.ihomey.linkuphome.device1.DevicesFragment"
        tools:layout="@layout/devices_fragment" >
        <action
            android:id="@+id/action_tab_devices_to_chooseDeviceTypeFragment"
            app:destination="@id/chooseDeviceTypeFragment" />
        <action
            android:id="@+id/action_tab_devices_to_c3ControlFragment"
            app:destination="@id/c3ControlFragment" />
        <action
            android:id="@+id/action_tab_devices_to_r2ControlFragment"
            app:destination="@id/r2ControlFragment" />
    </fragment>
    <fragment
        android:id="@+id/tab_zones"
        android:name="com.ihomey.linkuphome.zone.ZonesFragment"
        tools:layout="@layout/zones_fragment">
        <action
            android:id="@+id/action_tab_zones_to_chooseZoneTypeFragment"
            app:destination="@id/chooseZoneTypeFragment" />
        <action
            android:id="@+id/action_tab_zones_to_subZoneFragment"
            app:destination="@id/subZoneFragment" />
    </fragment>

    <fragment
        android:id="@+id/tab_setting"
        android:name="com.ihomey.linkuphome.setting.SettingFragment"
        tools:layout="@layout/setting_fragment">
        <action
            android:id="@+id/action_tab_setting_to_moreFragment"
            app:destination="@id/moreFragment" />
        <action
            android:id="@+id/action_tab_setting_to_zoneSettingFragment"
            app:destination="@id/zoneSettingFragment" />
        <action
            android:id="@+id/action_tab_setting_to_instructionsFragment"
            app:destination="@id/instructionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/moreFragment"
        android:name="com.ihomey.linkuphome.setting.MoreFragment"
        tools:layout="@layout/more_fragment"
        android:label="MoreFragment" >
        <action
            android:id="@+id/action_moreFragment_to_privacyAgreementFragment"
            app:destination="@id/privacyAgreementFragment" />
        <action
            android:id="@+id/action_moreFragment_to_termsOfUseFragment"
            app:destination="@id/termsOfUseFragment" />
    </fragment>

    <fragment
        android:id="@+id/createZoneFragment"
        android:name="com.ihomey.linkuphome.zone.CreateZoneFragment"
        android:label="create_zone_fragment"
        tools:layout="@layout/create_zone_fragment" >
    </fragment>

    <fragment
        android:id="@+id/shareZoneListFragment"
        android:name="com.ihomey.linkuphome.share1.ShareZoneListFragment"
        android:label="zone_share_list_fragment"
        tools:layout="@layout/zone_share_list_fragment" >
        <action
            android:id="@+id/action_shareZoneListFragment_to_shareZoneFragment"
            app:destination="@id/shareZoneFragment" />
    </fragment>

    <fragment
        android:id="@+id/shareZoneFragment"
        android:name="com.ihomey.linkuphome.share1.ShareZoneFragment"
        android:label="zone_share_fragment"
        tools:layout="@layout/zone_share_fragment" >
    </fragment>

    <fragment
        android:id="@+id/joinZoneFragment"
        android:name="com.ihomey.linkuphome.share1.JoinZoneFragment"
        android:label="zone_join_fragment"
        tools:layout="@layout/zone_join_fragment" >
    </fragment>

    <fragment
        android:id="@+id/zoneSettingFragment"
        android:name="com.ihomey.linkuphome.zone.ZoneSettingFragment"
        tools:layout="@layout/zone_setting_fragment"
        android:label="ZoneSettingFragment" >
        <action
            android:id="@+id/action_zoneSettingFragment_to_createZoneFragment2"
            app:destination="@id/createZoneFragment" />
        <action
            android:id="@+id/action_zoneSettingFragment_to_shareZoneListFragment"
            app:destination="@id/shareZoneListFragment" />
        <action
            android:id="@+id/action_zoneSettingFragment_to_joinZoneFragment"
            app:destination="@id/joinZoneFragment" />
    </fragment>

    <fragment
        android:id="@+id/instructionsFragment"
        android:name="com.ihomey.linkuphome.setting.InstructionsFragment"
        tools:layout="@layout/instructions_fragment"
        android:label="InstructionsFragment" />

    <fragment
        android:id="@+id/privacyAgreementFragment"
        android:name="com.ihomey.linkuphome.setting.PrivacyAgreementFragment"
        tools:layout="@layout/privacy_agreement_fragment"
        android:label="PrivacyAgreementFragment" />
    <fragment
        android:id="@+id/termsOfUseFragment"
        android:name="com.ihomey.linkuphome.setting.TermsOfUseFragment"
        tools:layout="@layout/terms_of_use_fragment"
        android:label="TermsOfUseFragment" />


    <fragment
        android:id="@+id/chooseZoneTypeFragment"
        android:name="com.ihomey.linkuphome.zone.ChooseZoneTypeFragment"
        tools:layout="@layout/choose_zone_type_fragment"
        android:label="ChooseZoneTypeFragment" />

    <fragment
        android:id="@+id/subZoneFragment"
        android:name="com.ihomey.linkuphome.zone.SubZoneFragment"
        android:label="sub_zone_fragment"
        tools:layout="@layout/sub_zone_fragment" >
        <action
            android:id="@+id/action_subZoneFragment_to_unBondedDevicesFragment2"
            app:destination="@id/unBondedDevicesFragment" />
    </fragment>

    <fragment
        android:id="@+id/unBondedDevicesFragment"
        android:name="com.ihomey.linkuphome.device1.UnBindedDevicesFragment"
        android:label="unbonded_devices_fragment"
        tools:layout="@layout/unbonded_devices_fragment" >
    </fragment>


    <fragment
        android:id="@+id/chooseDeviceTypeFragment"
        android:name="com.ihomey.linkuphome.device1.ChooseDeviceTypeFragment"
        tools:layout="@layout/choose_device_type_fragment"
        android:label="ChooseDeviceTypeFragment" >
        <action
            android:id="@+id/action_chooseDeviceTypeFragment_to_searchDeviceHintFragment2"
            app:destination="@id/searchDeviceHintFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchDeviceHintFragment"
        android:name="com.ihomey.linkuphome.device1.SearchDeviceHintFragment"
        tools:layout="@layout/search_device_hint_fragment"
        android:label="SearchDeviceHintFragment" >
        <action
            android:id="@+id/action_searchDeviceHintFragment_to_searchDeviceFragment2"
            app:destination="@id/searchDeviceFragment" />
        <action
            android:id="@+id/action_searchDeviceHintFragment_to_resetDeviceFragment2"
            app:destination="@id/resetDeviceFragment" />
    </fragment>
    <fragment
        android:id="@+id/resetDeviceFragment"
        android:name="com.ihomey.linkuphome.device1.ResetDeviceFragment"
        tools:layout="@layout/reset_device_fragment"
        android:label="ResetDeviceFragment" />
    <fragment
        android:id="@+id/searchDeviceFragment"
        android:name="com.ihomey.linkuphome.device1.SearchDeviceFragment"
        tools:layout="@layout/search_device_fragment"
        android:label="SearchDeviceFragment" >
        <action
            android:id="@+id/action_searchDeviceFragment_to_resetDeviceFragment2"
            app:destination="@id/resetDeviceFragment" />
        <action
            android:id="@+id/action_searchDeviceFragment_to_connectDeviceFragment2"
            app:destination="@id/connectDeviceFragment" />
    </fragment>
    <fragment
        android:id="@+id/connectDeviceFragment"
        android:name="com.ihomey.linkuphome.device1.ConnectDeviceFragment"
        tools:layout="@layout/connect_device_fragment"
        android:label="ConnectDeviceFragment" />



    <fragment
        android:id="@+id/c3ControlFragment"
        android:name="com.ihomey.linkuphome.control1.C3ControlFragment"
        android:label="c3_control_fragment"
        tools:layout="@layout/c3_control_fragment" >
    </fragment>

    <fragment
        android:id="@+id/r2ControlFragment"
        android:name="com.ihomey.linkuphome.control1.R2ControlFragment"
        android:label="r2_control_fragment"
        tools:layout="@layout/r2_control_fragment" >
        <action
            android:id="@+id/action_r2ControlFragment_to_r2SceneSettingFragment2"
            app:destination="@id/r2SceneSettingFragment" />
        <action
            android:id="@+id/action_r2ControlFragment_to_timerSettingFragment2"
            app:destination="@id/timerSettingFragment" />
    </fragment>

    <fragment
        android:id="@+id/r2SceneSettingFragment"
        android:name="com.ihomey.linkuphome.scene.R2SceneSettingFragment"
        android:label="r2_scene_setting_fragment"
        tools:layout="@layout/r2_scene_setting_fragment" >
    </fragment>

    <fragment
        android:id="@+id/timerSettingFragment"
        android:name="com.ihomey.linkuphome.time.TimerSettingFragment"
        android:label="fragment_timer_setting"
        tools:layout="@layout/fragment_timer_setting" >
    </fragment>

</navigation>